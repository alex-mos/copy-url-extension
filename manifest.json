{
  "background": {
    "scripts": ["assets/js/punycode.js", "assets/js/background.js"]
  },
  "commands": {
    "_execute_page_action": {
      "suggested_key": {
        "default": "Alt+X"
      }
    }
  },
  "content_scripts": [
    {
      "js": ["assets/js/content.js"],
      "matches": ["\u003Call_urls>"],
      "run_at": "document_start"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_description__",
  "icons": {
    "128": "assets/img/icon.png"
  },
  "manifest_version": 2,
  "name": "__MSG_name__",
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html"
  },
  "page_action": {
    "default_icon": {
      "128": "assets/img/icon.png"
    },
    "default_title": "__MSG_button_title__"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "clipboardWrite",
    "tabs",
    "alarms",
    "webRequest",
    "webRequestBlocking"
  ],
  "short_name": "copyurl",
  "version": "1.0.0"
}
